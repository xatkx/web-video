gris = #8F8F8F
grisli = #2e456b
ul.c
  list-style-type: none
  margin: 0
  padding: 0
  border-radius: 3px
  background-color: grisli
  top: 0
  width: 100%
  z-index: 2

li.b
  float: left

li a.a
  display: block
  padding: 15px
  color: #000
  text-align: center
  border-radius: 5px
  box-sizing: border-box
  text-decoration: none

li a.a:hover
  transition: 1s
  background-color: #000
  color: #fff

div.video
  position: relative
  margin-top: 50px
  margin-left: 10%
  margin-right: 10%
  width: 80%
  height: 100%
  background-color: #fff
  border-radius: 5px
  border: 1px inset #808080



.prin
  background: #fff
  margin: 70px

.ifram
  display: block
  margin-left: auto
  margin-right: auto
  width: 100%
  height: 500px
  border-style: inset
  border-radius: 5px

.cap
  font-size 0.9em
  
  

.body 
  text-transform: capitalize
  font-size 16px
  font-family arial
  background gris
